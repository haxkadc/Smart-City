<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="900.0" prefWidth="1500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Project.GraphicController">
   <children>
      <ImageView fitHeight="1079.0" fitWidth="1707.0" layoutX="-83.0" layoutY="21.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/FMIXB49.png" />
         </image>
      </ImageView>
      <Button layoutX="254.0" layoutY="665.0" mnemonicParsing="false" onAction="#printChart" text="POLLUTION">
         <font>
            <Font name="Tahoma Bold" size="24.0" />
         </font></Button>
      <Button layoutX="252.0" layoutY="733.0" mnemonicParsing="false" onAction="#printTemp" text="TEMPERATURE">
         <font>
            <Font name="Tahoma Bold" size="24.0" />
         </font></Button>
      <Button layoutX="251.0" layoutY="799.0" mnemonicParsing="false" onAction="#printCar" text="CAR">
         <font>
            <Font name="Tahoma Bold" size="24.0" />
         </font></Button>
      <BarChart fx:id="stackedBarChart" layoutX="155.0" layoutY="35.0" prefHeight="607.0" prefWidth="1381.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" side="LEFT" />
        </yAxis>
      </BarChart>
      <Button layoutX="1450.0" layoutY="688.0" mnemonicParsing="false" onAction="#HomeButton" text="HOME">
         <font>
            <Font name="Tahoma Bold" size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
